#!/bin/bash 
args=$@
# test first
curDir=`pwd`
testDir=$HOME
buildLog="/tmp/"$(USER)-log-x879873
SelfTestDir=`which runSelfTest`
if [ "$SelfTestDir" == ""]; then
    exit 1
fi
cd $SelfTestDir
./runSelfTest 
ret=$?

if ["$ret" == "1"]; then 
	exit 1
fi 
# if  test pass, then commit
echo svn ci $@

# test after ci
./runSelfRegress
ret=$?
if ["$ret" == "1"]; then 
	exit 1
fi 
exit 0
